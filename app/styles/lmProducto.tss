	separatorStyle: "NONE"
}


"#rowContainer":{
	borderColor: "#cacdd8",
	borderRadius: 5,
	borderWidht: 1,
	left: "5",
	right: "5",
	bottom: "5",
	top: "5",
	height: Ti.UI.SIZE,
	width: Ti.UI.FILL,
	backgroundColor: "#fff",	
	layout: "vertical"
} 

"#infoNutrimental":{
	width: Ti.UI.FILL,
	height: "150",		
